{
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "scripts": {
    "dev": "node ./app.js",
    "test-mocha": "mocha",
    "mocha": "nyc --reporter=html --reporter=text mocha",
    "test": "cross-env NODE_ENV=test nyc mocha --exit"
  },
  "dependencies": {
    "busboy": "^0.2.14",
    "co-wechat-oauth": "^2.0.1",
    "cos-nodejs-sdk-v5": "^2.4.8",
    "ejsexcel": "^3.5.2",
    "eslint": "^5.3.0",
    "eslint-plugin-import": "^2.13.0",
    "fanqier-wxredpack": "^1.0.0",
    "ioredis": "^3.2.2",
    "ioredis-lock": "^3.4.0",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa2-cors": "^2.0.6",
    "move-file": "^1.0.0",
    "node-xlsx": "^0.11.2",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "ioredis-lock": "^3.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "koa-jwt": "^3.3.2",
    "log4js": "^3.0.4",
    "mocha": "^4.0.1",
    "mochawesome": "^3.0.3",
    "moment": "^2.22.2",
    "mysql2": "^1.6.1",
    "nyc": "^13.0.1",
    "sequelize": "^4.38.0",
    "source-map-support": "^0.5.6",
    "supertest": "^3.1.0"
  }
